{
    "name": "conquest",
    "displayName": "Conquest",
    "version": "0.0.1",
    "description": "Conquer the hassle of registering for USC classes.",
    "author": "Brennen Ho",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "package": "plasmo package"
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fontsource/inter": "^5.0.18",
        "@fontsource/open-sans": "^5.0.28",
        "@fontsource/roboto": "^5.0.13",
        "@mantine/core": "^7.10.2",
        "@mantine/hooks": "^7.10.2",
        "@mantine/notifications": "^7.11.2",
        "@mui/icons-material": "^5.15.20",
        "@mui/styles": "^5.15.20",
        "@plasmohq/messaging": "^0.6.2",
        "@plasmohq/storage": "^1.10.0",
        "@tabler/icons-react": "^3.6.0",
        "axios": "^1.7.2",
        "boxicons": "^2.1.4",
        "cheerio": "1.0.0-rc.12",
        "classnames": "^2.5.1",
        "jquery": "^3.7.1",
        "plasmo": "0.88.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-ratings-declarative": "^3.4.1"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.1.1",
        "@types/chrome": "0.0.258",
        "@types/node": "20.11.5",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "prettier": "3.2.4",
        "typescript": "5.3.3"
    },
    "manifest": {
        "host_permissions": [
            "https://webreg.usc.edu/*"
        ],
        "permissions": [
            "tabs",
            "notifications"
        ],
        "web_accessible_resources": [
            {
                "resources": [
                    "assets/icon.png"
                ],
                "matches": [
                    "<all_urls>"
                ]
            }
        ]
    }
}
